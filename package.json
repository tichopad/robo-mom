{
	"name": "robo-mom",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"check": "pnpm run check:biome && pnpm run check:ts",
		"check:biome": "biome check --fix .",
		"check:ts": "tsc",
		"db:migrate": "drizzle-kit generate && node --env-file=.env ./src/db/migrate.ts",
		"db:studio": "drizzle-kit studio --verbose",
		"start": "node --env-file=.env ./src/main.ts",
		"cli": "node --env-file=.env ./src/cli.ts"
	},
	"packageManager": "pnpm@10.2.0",
	"dependencies": {
		"@ai-sdk/openai": "1.3.16",
		"@electric-sql/pglite": "0.2.17",
		"@huggingface/transformers": "3.5.0",
		"@std/collections": "npm:@jsr/std__collections@^1.0.10",
		"@std/front-matter": "npm:@jsr/std__front-matter@^1.0.9",
		"@std/fs": "npm:@jsr/std__fs@^1.0.16",
		"@std/text": "npm:@jsr/std__text@^1.0.12",
		"@xenova/transformers": "2.17.2",
		"ai": "4.3.9",
		"citty": "^0.1.6",
		"consola": "^3.4.2",
		"drizzle-kit": "0.31.0",
		"drizzle-orm": "0.42.0",
		"onnxruntime-node": "^1.21.1",
		"zod": "3.24.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "22.14.1",
		"typescript": "5.8.3"
	}
}
