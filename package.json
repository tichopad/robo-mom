{
	"name": "robo-mom",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"check": "pnpm run check:biome && pnpm run check:ts",
		"check:biome": "biome check --fix .",
		"check:ts": "tsc",
		"db:migrate": "drizzle-kit generate && tsx --env-file=.env ./src/db/migrate.ts",
		"db:cli": "tsx --env-file=.env ./src/db-cli.ts",
		"start": "tsx --env-file=.env ./src/main.ts",
		"cli": "tsx --env-file=.env ./src/cli.ts",
		"ink:new": "tsx --watch --env-file=.env ./src/app.tsx"
	},
	"packageManager": "pnpm@10.12.1",
	"dependencies": {
		"@ai-sdk/openai": "1.3.22",
		"@electric-sql/pglite": "0.3.0",
		"@huggingface/transformers": "3.5.2",
		"@std/collections": "npm:@jsr/std__collections@1.0.10",
		"@std/front-matter": "npm:@jsr/std__front-matter@1.0.9",
		"@std/fs": "npm:@jsr/std__fs@1.0.16",
		"@std/text": "npm:@jsr/std__text@1.0.12",
		"@xenova/transformers": "2.17.2",
		"ai": "4.3.16",
		"citty": "0.1.6",
		"drizzle-kit": "0.31.1",
		"drizzle-orm": "0.44.2",
		"ink": "6.0.0",
		"onnxruntime-node": "1.21.1",
		"react": "19.1.0",
		"tsx": "4.19.4",
		"valtio": "2.1.5",
		"winston": "3.17.0",
		"zod": "3.25.56"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "22.15.30",
		"@types/react": "19.1.6",
		"typescript": "5.8.3"
	}
}
